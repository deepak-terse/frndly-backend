        "migrationDown": "ts-node ./node_modules/typeorm/cli.js migration:revert"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "mysql": "^2.18.1",
        "ts-node": "^9.1.1",
        "typeorm": "^0.2.31"
    },
    "devDependencies": {
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/node": "^14.14.31",
        "typescript": "^4.2.2"
    }
}
